<CALLBACK  type="HASH">
	<METHODS>
		<METHOD>
			<PARAM name="callback">
				<DESC>Callback to contain returned property values</DESC>
				<POSSIBILITY>
					<DESC>Function to receive property values</DESC>
					<TYPE>
						<FUNCTION>
							<FUNC_PARAM>
								<DESC>Mapping of property names to Values</DESC>
								<TYPE>HASH</TYPE>
							</FUNC_PARAM>
						</FUNCTION>	
					</TYPE>
				</POSSIBILITY>
				<POSSIBILITY>
					<DESC>URL</DESC>
					<TYPE>String</TYPE>
				</POSSIBILITY>
				<POSSIBILITY>
					<DESC>Synchronous Operation</DESC>
					<TYPE>nil</TYPE>
				</POSSIBILITY>
			</PARAM>
            
			<PARAM name="callbackParam">
				<DESC>Application specific parameter</DESC>
                <TYPE>String</TYPE>

                <CAN_BE_NIL>
                    <DESC>Can be missed</DESC>
                </CAN_BE_NIL>
            </PARAM>
		</METHOD>
	</METHODS>
</CALLBACK>
