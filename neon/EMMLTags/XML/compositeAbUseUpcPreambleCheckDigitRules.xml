<?xml version = "1.0"?>
<?xml-stylesheet type="text/xsl" href="pb_help.xsl"?>

<HELPPAGE>
	<FUNC>
		<NAME>compositeAbUseUpcPreambleCheckDigitRules</NAME>
		<PATH>Barcode Scanner\Decoder and Parameter Tags\CompositeAb</PATH>
		<TYPE>Decoder Setting</TYPE>
		<SYNOPSIS>causes the UPC rules specified in the UPC EAN parameters to be used when reporting composite decode data.</SYNOPSIS>
		<SYNTAX>
            		<USAGE>
               		 <EXAMPLE>&lt;META HTTP-Equiv="scanner" content="compositeAbUseUpcPreambleCheckDigitRules:[parameter]"&gt;</EXAMPLE>
            		</USAGE>
        	</SYNTAX>
		<PARAMS_WITH_ATTRIBUTES>
			<PARAM>
				<NAME>compositeAbUseUpcPreambleCheckDigitRules</NAME>
                		<VALUES>true / false</VALUES>
				<DESC>Enables / Disables the property.</DESC>
				<DEFAULT>Device specific</DEFAULT>				
				<SCANNERTAG>TRUE</SCANNERTAG>
			</PARAM>
        	</PARAMS_WITH_ATTRIBUTES>
		<EXAMPLES>
			<EXAMPLE>
				<DESC>The following example enables the scanner to read barcodes with the compositeAbUseUpcPreambleCheckDigitRules property set</DESC>
				<CODE>
&lt;META HTTP-Equiv="scanner" Content="compositeAb:enabled"&gt;
&lt;META HTTP-Equiv="scanner" Content="compositeAbUseUpcPreambleCheckDigitRules:true"&gt;
&lt;META HTTP-Equiv="scanner" Content="enabled"&gt;
				</CODE>
			</EXAMPLE>
		</EXAMPLES>
		<ADDITIONAL>
			<PLATFORM>All supported devices except: Android.</PLATFORM>
			<APPLIES>Transient - Decoder settings are only guaranteed to persist until the Scanner is disabled</APPLIES>
			<REQ>Scanner or Imager module and device that supports CompositeAB.</REQ>
			<VER_INTRODUCED>1.0.0</VER_INTRODUCED>
		</ADDITIONAL>
		<SEEALSO>
			<TOPIC RELPATH ="../../">Scanner</TOPIC>
		</SEEALSO>
	</FUNC>
</HELPPAGE>
